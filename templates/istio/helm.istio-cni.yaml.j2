profile: ambient
{%- if istio_revision %}
revision: {{ istio_revision }}
{%- endif %}
global:
  hub: {{ istio_repo }}
  tag: {{ istio_ver }}{{ istio_flavor | default('',true) }}
  variant: {{ istio_variant }}
ambient:
  dnsCapture: true
